{
  "name": "kwork-backend",
  "version": "1.0.0",
  "description": "Backend API for Kwork monument store",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "build": "echo 'No build process needed for pure Node.js'",
    "migrate": "node src/scripts/migrate.js",
    "seed": "node src/scripts/seed.js",
    "setup": "npm run migrate && npm run seed",
    "export-data": "node src/scripts/export-data.js",
    "import-data": "node src/scripts/import-data.js",
    "import-all": "node src/scripts/import-all-data.js",
    "deploy-ssh": "chmod +x deploy-ssh.sh && ./deploy-ssh.sh",
    "pm2:start": "pm2 start ecosystem.config.json",
    "pm2:stop": "pm2 stop stonerose-backend",
    "pm2:restart": "pm2 restart stonerose-backend",
    "pm2:status": "pm2 status",
    "pm2:logs": "pm2 logs stonerose-backend"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "puppeteer": "^24.30.0",
    "ssh2": "^1.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "api",
    "monuments",
    "postgresql",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT"
}
